
@model List<Thinh.Models.Feedbacks>
@{
    ViewData["Title"] = "FeedbackList";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>FeedbackList</h2>

@{ if (Model.Count == 0)
    {
    <h4>Your feedback list is empty.</h4>
    }
    else
        { <div class="row">
            <div class="col-sm-2">
                <b>Email</b>
            </div>
            <div class="col-sm-2">
                <b>Feedback</b>
            </div>
        </div>
        foreach (var item in Model)
        {
            <div class="row">
                <div class="col-sm-2">
                    @item.Email
                </div>
                <div class="col-sm-2">
                    @item.Feedback
                </div>
            </div>
        }
    }
}


<a class="btn btn-primary" asp-action="Index">Back to Home Page</a>

